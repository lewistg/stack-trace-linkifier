{
  "manifest_version": 2,
  "name": "Stack Trace Links",
  "description": "Turn stack frames in JavaScript stack trace text into clickable links.",
  "version": "1.0",
  "permissions": ["tabs", "<all_urls>"],
  "minimum_chrome_version": "10.0",
  "background": {
      "persistent": false,
      "scripts": ["background.js"]
  },
  "browser_action": {
     "default_icon": {
         "32":  "icon32.png"
     }
  },
  "devtools_page": "devtools.html"
}
